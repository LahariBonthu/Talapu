        "dotenv": "^16.5.0",
        "express": "^4.18.2",
        "mongoose": "^8.14.1",
        "mongose": "^0.0.2-security",
        "morgan": "^1.10.0",
        "multer": "^1.4.5-lts.2",
        "multer-storage-cloudinary": "^4.0.0",
        "nodemon": "^3.1.10",
        "react": "^19.1.0",
        "react-dom": "^19.1.0",
        "react-router-dom": "^6.30.0"
    },
    "devDependencies": {
